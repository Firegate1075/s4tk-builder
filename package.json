{
  "name": "s4tk-builder",
  "version": "0.1.1",
  "displayName": "Sims 4 Toolkit Build System",
  "description": "Build system for Sims 4 Toolkit projects. This command line tool implements the build system of the S4TK Visual Studio Code extension.",
  "private": false,
  "license": "GPL-3.0",
  "author": "Firegate1075",
  "repository": {
    "type": "git",
    "url": "https://github.com/Firegate1075/s4tk-builder"
  },
  "bugs": {
    "url": "https://github.com/Firegate1075/s4tk-builder/issues"
  },
  "scripts": {
    "prepare": "npm run compile",
    "compile": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "watch": "tsc -w -p ./"
  },
  "dependencies": {
    "@oclif/core": "^4.5.0",
    "@s4tk/hashing": "^0.2.1",
    "@s4tk/models": "^0.6.14",
    "@s4tk/xml-dom": "^0.2.6",
    "glob": "^10.2.2",
    "jsonschema": "^1.4.1",
    "jszip": "^3.10.1",
    "lru-cache": "^11.1.0",
    "uuid": "^9.0.0",
    "vscode-uri": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/uuid": "^9.0.1",
    "@types/vscode": "^1.73.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.4",
    "typescript": "^5.0.2"
  },
  "categories": [
    "Other"
  ],
  "bin": {
    "s4tk-builder": "./bin/run.js"
  },
  "oclif": {
    "bin": "s4tk-builder",
    "commands": "./out/commands",
    "dirname": "s4tk-builder",
    "topicSeparator": " ",
    "helpOptions": {
      "flagSortOrder": "none",
      "showFlagOptionsInTitle": false
    }
  }
}
